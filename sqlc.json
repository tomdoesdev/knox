{
  "version": "2",
  "sql": [
    {
      "engine": "sqlite",
      "queries": "./db/queries",
      "schema": "./db/schema.sql",
      "gen": {
        "go": {
          "package": "database",
          "out": "internal/database",
          "emit_json_tags": true,
          "emit_prepared_queries": false,
          "emit_interface": true
        }
      }
    }
  ]
}